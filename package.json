    "crypto": "./lib/utils/browser-crypto.js"
  },
  "bugs": {
    "url": "https://github.com/sockjs/sockjs-client/issues"
  },
  "contributors": [
    {
      "name": "Bryce Kahle",
      "email": "bkahle@gmail.com"
    },
    {
      "name": "Marek Majkowski",
      "email": "deadbeef@popcount.org"
    }
  ],
  "dependencies": {
    "faye-websocket": "~0.7.3",
    "json3": "^3.3.2"
  },
  "devDependencies": {
    "browserify": "^4.2.3",
    "debug": "^2.0.0",
    "exorcist": "^0.1.6",
    "expect.js": "~0.3.1",
    "gulp": "^3.8.7",
    "gulp-eslint": "~0.1.8",
    "gulp-mocha": "~1.1.0",
    "minifyify": "^3.0.12",
    "mocha": "^1.21.4",
